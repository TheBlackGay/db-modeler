VITE_APP_TITLE=数据建模协作平台
VITE_APP_API_BASE_URL=http://localhost:5001/api/v1
VITE_APP_WS_URL=ws://localhost:5001

# 开发环境配置
VITE_APP_ENV=development
VITE_APP_PORT=3001

# 性能监控
VITE_ENABLE_PERFORMANCE_MONITORING=true

# 调试配置
VITE_APP_DEBUG=true
VITE_APP_LOG_LEVEL=debug

# 版本信息
VITE_APP_VERSION=0.1.0
VITE_APP_BUILD_TIME=${BUILD_TIME}

# 第三方服务配置
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# 特性开关
VITE_FEATURE_COLLABORATION=true
VITE_FEATURE_VERSION_CONTROL=true
VITE_FEATURE_AI_SUGGESTIONS=false
